<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ForegroundImageView">
        <attr name="android:foreground"/>
    </declare-styleable>

    <declare-styleable name="PokemonTypeView">
        <attr name="pokeType" format="enum">
            <enum name="colorless" value="0" />
            <enum name="fire" value="1" />
            <enum name="grass" value="2" />
            <enum name="water" value="3" />
            <enum name="electric" value="4" />
            <enum name="fighting" value="5" />
            <enum name="psychic" value="6" />
            <enum name="steel" value="7" />
            <enum name="dragon" value="8" />
            <enum name="fairy" value="9" />
            <enum name="darkness" value="10" />
        </attr>
    </declare-styleable>

</resources>